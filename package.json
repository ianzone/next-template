{
  "name": "react-next",
  "description": "Next.js template with TypeScript, Tailwind CSS, Biome, and Faker.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --fix src --reporter summary",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "postinstall": "lefthook install",
    "commit": "git add -A && git-cz"
  },
  "dependencies": {
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "git-cz": "^4.9.0",
    "happy-dom": "^14.12.3",
    "lefthook": "^1.6.18",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  }
}
